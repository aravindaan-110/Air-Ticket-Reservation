<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true">
        <shape>
            <solid
                android:color="#F2385F"/>
            <corners
                android:radius="30dp"/>

        </shape>
    </item>
    <item>
        <shape >
            <gradient
                android:startColor="@color/red"
                android:endColor="#2577F2"
                android:angle="270"/>

            <corners
                android:radius="30dp"/>

        </shape>
    </item>


</selector>

