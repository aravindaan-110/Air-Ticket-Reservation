<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.airplaneticketreservation">

    <application
        android:allowBackup="true"

        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ticketActivity" />
        <activity android:name=".billingActivity" />
        <activity android:name=".flightActivity" />
        <activity android:name=".airplaneActivity" />
        <activity android:name=".viewActivity" />
        <activity android:name=".cancelActivity" />
        <activity android:name=".bookingActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".loginActivity" />
        <activity android:name=".registerActivity" />
        <activity android:name=".startActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>